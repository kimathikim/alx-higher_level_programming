<!DOCTYPE html>
<html>
<head>
  <title>GET method</title>
</head>
<body>
  <form id="myForm">
    Name: <input type="text" name="fname" id="fname">
    <input type="submit" name="submit" value="submit">
  </form>

    <div id="result">
      
    </div>
    <script>men
    function submitForm() {

      var formData = new  formData(document.getElementById("myForm"));
      var obj = new XMLHttpRequest();

      obj.onreadystatechange = function() {
      if (obj.readyState === XMLHttpRequest.DONE) {
      if (obj.status === 200) {
      document.getElementById("Result").innerHTML = obj.responseText;
      } else {
      document.getElementById("result").innerHTML = "Error: " + obj.status;

      }
      }
      }

      obj.open("POST", "data.php", true);
      obj.send(formData)
    }
    </script>
</body>
</html>

